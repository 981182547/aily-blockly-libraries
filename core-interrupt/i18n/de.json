{
"toolbox_name": "Interrupt",
"io_attach_interrupt": {
    "message0": "Hardware-Interrupt Pin # %1",
    "args0": [
        {
            "options": "${board.interruptPins}"
        }
    ],
    "message1": "Modus %1",
    "args1": [
        {
            "options": "${board.interruptMode}"
        }
    ],
    "message2": "%1"
},
"io_detach_interrupt": {
    "message0": "Hardware-Interrupt aufheben Pin # %1",
    "args0": [
        {
            "options": "${board.interruptPins}"
        }
    ]
}
}