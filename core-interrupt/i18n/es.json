{
"toolbox_name": "Interrupción",
"io_attach_interrupt": {
    "message0": "Interrupción de hardware Pin # %1",
    "args0": [
        {
            "options": "${board.interruptPins}"
        }
    ],
    "message1": "Modo %1",
    "args1": [
        {
            "options": "${board.interruptMode}"
        }
    ],
    "message2": "%1"
},
"io_detach_interrupt": {
    "message0": "Cancelar interrupción de hardware Pin # %1",
    "args0": [
        {
            "options": "${board.interruptPins}"
        }
    ]
}
}